@Article{Ghosh2011,
  Title                    = {Software for systems biology: from tools to integrated platforms},
  Author                   = {Ghosh, Samik and Matsuoka, Yukiko and Asai, Yoshiyuki and Hsin, Kun-Yi and Kitano, Hiroaki},
