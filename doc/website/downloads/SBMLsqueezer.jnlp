<jnlp spec="1.0+" codebase="http://www.cogsys.cs.uni-tuebingen.de/software/SBMLsqueezer/downloads" href="SBMLsqueezer.jnlp">
    <information>
        <title>SBMLsqueezer</title>
        <vendor>Center for Bioinformatics Tuebingen (ZBIT)</vendor>
        <description>SBMLsqueezer - a generator of kinetic rate equations for biological models</description>
        <description kind="short">SBMLsqueezer</description>
        <homepage href="http://www.cogsys.cs.uni-tuebingen.de/software/SBMLsqueezer"/>
        <icon href="http://www.cogsys.cs.uni-tuebingen.de/software/SBMLsqueezer/img/SBMLsqueezerIcon_48.png"/>
        <icon kind="shortcut" href="http://www.cogsys.cs.uni-tuebingen.de/software/SBMLsqueezer/img/SBMLsqueezerIcon_16.png" width="16" height="16"/>
        <icon kind="splash" href="http://www.cogsys.cs.uni-tuebingen.de/software/SBMLsqueezer/img/SBMLsqueezerSplash.gif"/>
        <offline-allowed/>
    </information>
    <security>
      <all-permissions/>
    </security>
    <resources>
      <j2se version="1.6+" initial-heap-size="64m" max-heap-size="256m" java-vm-args="-splash:org/sbml/squeezer/resources/img/SBMLsqueezerSplash.gif" href="http://java.sun.com/products/autodl/j2se"/>
      <jar href="SBMLsqueezer_v2.0_incl-libs.jar" main="true"/>
    </resources>
    <application-desc name="SBMLsqueezer" main-class="org.sbml.squeezerr.SBMLsqueezer"/>
    <update check="background"/>
</jnlp>
