    <listOfRules>
      <assignmentRule metaid="metaid_0000033" variable="Ratebasal_PIPsyn">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 0.581 </cn>
            <ci> PIPsynbasal_Stimulation </ci>
            <ci> kBasalSynPIP </ci>
            <apply>
              <plus/>
              <cn type="integer"> -1 </cn>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <apply>
                    <plus/>
                    <ci> PIP_basal </ci>
                    <apply>
                      <minus/>
                      <ci> PIP_PM </ci>
                    </apply>
                  </apply>
                  <apply>
                    <divide/>
                    <cn type="integer"> 1 </cn>
                    <ci> PIP_basal </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000062" variable="Ratestim_PIPsyn">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> kStimSynPIP </ci>
            <apply>
              <exp/>
              <apply>
                <minus/>
                <apply>
                  <times/>
                  <apply>
                    <plus/>
